<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" version="3.1">
    <display-name>Car Web Application</display-name>
    
    <servlet>
        <servlet-name>CarServlet</servlet-name>
        <servlet-class>com.carwebapp.servlet.CarServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>CarServlet</servlet-name>
        <url-pattern>/car</url-pattern>
    </servlet-mapping>
</web-app>
